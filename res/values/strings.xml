<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!-- used in various places to indicate how many songs for a given category. -->
    <plurals name="Nsongs">
        <item quantity="one">1 song</item>
        <item quantity="other"><xliff:g id="count">%d</xliff:g> songs</item>
    </plurals>

    <!-- Used in artists list view, indicates how many albums exist for a given artist. -->
    <plurals name="Nalbums">
        <!-- number of albums is one -->
        <item quantity="one">1 album</item>
        <!-- number of albums is not equal to one -->
        <item quantity="other"><xliff:g id="count">%d</xliff:g> albums</item>
    </plurals>

    <!--
    This string is used as the format string in a String.format call,
    and 5 additional arguments are available for printing:
    1 - hours
    2 - minutes
    3 - minutes%60
    4 - seconds
    5 - second%60
    -->
    <skip/>
    <!-- Do not translate. Duration format for duration < 1 hour -->
    <string translatable="false" name="durationformatshort"><xliff:g id="format">%2$d:%5$02d</xliff:g></string>
    <!-- Do not translate. Duration format for duration >= 1 hour -->
    <string translatable="false" name="durationformatlong"><xliff:g id="format">%1$d:%3$02d:%5$02d</xliff:g></string>

    <!-- Menu item that deletes the currently selected song.
         The user will be prompted to confirm before deletion actually takes place -->
    <string name="delete_item">Delete</string>
    <!-- Menu item that deletes all items in the currently selected category.
         The user will be prompted to confirm before deletion actually takes place -->
    <string name="delete_all">Delete all</string>
    <!-- Menu item to play back all the songs in the currently showing list in list order now-->
    <string name="play_all_now">Play all now</string>
    <!-- Menu item to play back all the songs in the currently showing list in list order after current song -->
    <string name="play_all_next">Play all next</string>
    <!-- Menu item to play back all the songs in the currently showing list in list order after current play queue -->
    <string name="queue_all">Queue all</string>
    <!-- Menu item to play back all the songs in the currently showing list in list order interleaved with the current play queue -->
    <string name="interleave_all">Interleave</string>
    <string name="interleaveNNN">Queue <xliff:g id="queue">%d</xliff:g> - list <xliff:g id="list">%d</xliff:g></string>
    <!-- Menu item to shuffle all songs in play queue or play list -->
    <string name="shuffle">Shuffle</string>
    <!-- Menu item to uniqueify all songs in play queue or play list -->
    <string name="uniqueify">Remove dups</string>

